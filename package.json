{
  "name": "@lcooper/eslint-config",
  "version": "1.0.0",
  "description": "Shareable config for ESLint",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luciancooper/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "author": "Lucian Cooper <cooperlucian@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luciancooper/eslint-config/issues"
  },
  "homepage": "https://github.com/luciancooper/eslint-config#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@lcooper"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@lcooper/commitlint-config": "^1.0.0",
    "husky": "^3.1.0"
  }
}
