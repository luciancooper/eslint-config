{
  "name": "@lcooper/eslint-config-jest",
  "version": "1.1.0",
  "description": "Shareable ESLint config for Jest tests",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint ."
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.js",
    "config.js"
  ],
  "keywords": [
    "eslint",
    "eslintconfig",
    "jest"
  ],
  "author": "Lucian Cooper <cooperlucian@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luciancooper/eslint-configs.git",
    "directory": "packages/eslint-config-jest"
  },
  "bugs": {
    "url": "https://github.com/luciancooper/eslint-configs/issues"
  },
  "homepage": "https://github.com/luciancooper/eslint-configs/tree/main/packages/eslint-config-jest#readme",
  "jest": {
    "displayName": "eslint-config-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "../../setupTests.js"
    ]
  },
  "peerDependencies": {
    "eslint": ">=7.3",
    "eslint-plugin-jest": ">=24.3"
  }
}
