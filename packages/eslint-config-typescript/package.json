{
  "name": "@lcooper/eslint-config-typescript",
  "version": "1.0.1",
  "description": "Shareable ESLint config for Typescript projects",
  "keywords": [
    "eslint",
    "eslintconfig",
    "typescript"
  ],
  "main": "index.js",
  "files": [
    "index.js",
    "rules"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint ."
  },
  "publishConfig": {
