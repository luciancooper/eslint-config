    "eslint": ">=7.3",
    "eslint-plugin-import": ">=2.23.0",
    "eslint-plugin-jsdoc": ">=35.0.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "typescript": "^4.3.2"
  },
  "peerDependenciesMeta": {
