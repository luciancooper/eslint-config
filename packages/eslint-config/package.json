{
  "name": "@lcooper/eslint-config",
  "version": "1.0.0",
  "description": "Shareable config for ESLint",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint ."
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "index.js",
    "browser.js"
  ],
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "author": "Lucian Cooper <cooperlucian@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luciancooper/eslint-configs.git",
    "directory": "packages/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/luciancooper/eslint-configs/issues"
  },
  "homepage": "https://github.com/luciancooper/eslint-configs/packages/eslint-config#readme",
  "jest": {
    "displayName": "eslint-config",
    "verbose": true,
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/__tests__/*.[jt]s?(x)"
    ]
  },
  "dependencies": {
    "eslint-config-airbnb-base": "^14.2.0"
  },
  "peerDependencies": {
    "eslint": ">= 4",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsdoc": "^30.3.0"
  }
}
